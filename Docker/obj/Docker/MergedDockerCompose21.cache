name: dockercompose12801568083792086672
services:
  authserver.web:
    build:
      context: /Users/igor/Projects/AuthServer1/Docker
      dockerfile: ../AuthServer.Web/Dockerfile
    container_name: AuthServer
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      ASPNETCORE_Kestrel__Certificates__Default__Password: "33731801"
      ASPNETCORE_Kestrel__Certificates__Default__Path: /app/cert/cert.pfx
      ASPNETCORE_URLS: https://+:443#;http://+:80
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "80"
      protocol: tcp
    - mode: ingress
      target: 443
      published: "4000"
      protocol: tcp
    volumes:
    - type: bind
      source: /Users/igor/Projects/AuthServer1/Docker/certificates
      target: /app/cert
      bind:
        create_host_path: true
    - type: bind
      source: /Users/igor/Projects/AuthServer1/Docker/application
      target: /app
      bind:
        create_host_path: true
  sql-server:
    container_name: sql-server
    environment:
      ACCEPT_EULA: "1"
      MSSQL_SA_PASSWORD: SU2orange!
    image: mcr.microsoft.com/azure-sql-edge
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
    volumes:
    - type: bind
      source: /Users/igor/Projects/AuthServer1/Docker/mssql/data
      target: /var/opt/mssql/data
      bind:
        create_host_path: true
    - type: bind
      source: /Users/igor/Projects/AuthServer1/Docker/mssql/log
      target: /var/opt/mssql/log
      bind:
        create_host_path: true
    - type: bind
      source: /Users/igor/Projects/AuthServer1/Docker/mssql/secrets
      target: /var/opt/mssql/secrets
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose12801568083792086672_default